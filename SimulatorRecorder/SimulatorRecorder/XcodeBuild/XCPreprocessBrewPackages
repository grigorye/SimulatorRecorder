dn=$(dirname "$0")

homebrewDir="${GE_HOST_HOMEBREW_DIR:?}"

chmod -R +w "${homebrewDir:?}"
"${dn:?}"/update-homebrew-bundle-lib-names.rb -v "${homebrewDir:?}"
