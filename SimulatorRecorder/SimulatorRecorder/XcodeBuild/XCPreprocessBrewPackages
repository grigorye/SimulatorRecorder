exit 0

dn=$(dirname "$0")

homebrewDir="${CONFIGURATION_BUILD_DIR:?}/${TARGET_NAME:?}-homebrew"

chmod -R +w "${homebrewDir:?}"
"${dn:?}"/update-homebrew-bundle-lib-names.rb -v "${homebrewDir:?}"
